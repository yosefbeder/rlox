class Person {
  constructor(name, age, school) {
    this.name = name;
    this.age = age;
    this.school = school;
  }

  introduce() {
    print("I'm " + this.name + ", I'm " + this.age + ", and I study at " + this.school);
  }
}

var person = Person("Ali", 20, "Cambridge");

person.introduce();

