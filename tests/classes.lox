class Person {
  init(name, age, school) {
    this.name = name;
    this.age = age;
    this.school = school;
  }

  introduce() {
    print("I'm " + this.name + ", I'm " + this.age + ", and I study at " + this.school);
  }
}

var ali = Person("Ali", 20, "Cambridge");
var yosef = Person("Yosef", 16, "Cairo University");

yosef.introduce_ali = ali.introduce;

yosef.introduce_ali();
ali.introduce();

var new_student = yosef.init("Mostafa", 17, "Cairo University");

print(new_student.name);
print(yosef.name);
