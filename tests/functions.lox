fun do_fun_stuff(return_something) {
  for(var i = 0; i < 20; i = i + 1) {
    if (i == 10 and return_something) return "i is 10"; 
    print(i);
  }
}

print(do_fun_stuff(true));
print(do_fun_stuff(false));

fun make_counter() {
  var i = 0;

  return fun () {
    i = i + 1;
    print(i);
  };
}

var counter = make_counter();
counter(); // "1"
counter(); // "2"

fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 20; i = i + 1) {
  print(fib(i));
}

var lamda = fun () {
  print("lamda");
};
lamda();

(fun() {
  print("IIFE");
})();