fun do_fun_stuff(return_something) {
  for(var i = 0; i < 20; i = i + 1) {
    if (i == 10 and return_something) return "i is 10"; 
    print(i);
  }
}

print(do_fun_stuff(true));
print(do_fun_stuff(false));

fun parent() {
  fun child() {
    print("I'm a good child");
  }

  return child;
}

// Don't ask me how 😏
var child = parent();

child();