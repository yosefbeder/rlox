fun make_counter() {
  var i = 0;

  return fun () {
    i = i + 1;
    print(i);
  };
}

var counter = make_counter();
counter();
counter();

fun fib(n) {
  if (n <= 1) return n;
  return fib(n - 2) + fib(n - 1);
}

for (var i = 0; i < 20; i = i + 1) {
  print(fib(i));
}

